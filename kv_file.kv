                        password:True
                        color_mode: 'custom'
                        # line_color_focus: 0, 0, 1, 1
                        line_color_focus:app.theme_cls.primary_color

            MDBoxLayout:
                orientation:'vertical'
                size_hint_y:.6
                # md_bg_color:.8,.8,.2,1
                MDBoxLayout:
                    # size_hint_y:.4
                    orientation: 'vertical'
                    MDCard:
                        # text:'LOGIN'
                        # pos_hint:{'top':.4}
                        size_hint:1,None
                        height:dp(50)
                        radius:dp(10)
                            # theme_text_color:'Custom'
                        # md_bg_color:0,0,1,1
                        md_bg_color:app.theme_cls.primary_color
                        on_press:
                            app.register(register_user_name.text,register_email.text,register_pass.text,register_pass_confirm.text,register_spinner)
                            
                        MDLabel:
                            text:'Create Account'
                            font_name:login_font
                            halign:'center'
                            font_size:sp(26)
                            theme_text_color:'Custom'
                            text_color:1,1,1,1
            
                    FloatLayout:
                        size_hint_y:.1
                        
                        MDSpinner:        
                            id:register_spinner
                            size_hint:None,None
                            size:dp(30),dp(30)  
                            active:False
                            pos_hint:{'center_y':.5,'center_x':.5}

                AnchorLayout:
                    size_hint_y:.05
                    anchor_x:'right'
                    # md_bg_color:.2,.5,.8,1
                    MDTextButton:
                        text:'Login?'
                        theme_text_color:'Custom'
                        # text_color:0,0,1,1
                        text_color:app.theme_cls.primary_color
                        # text_color:.6,0,.8,1
                        font_size:sp(20)
                        on_press:
                            root.manager.transition=SlideTransition() 
                            root.manager.transition.direction='right'
                            root.manager.current='LoginPage'
                    
<ResetPage@Screen>
    name:'ResetPage'
    MDGridLayout:
        cols:1
        padding:dp(20),dp(50)
        spacing: '20dp'
        MDCard:
            padding:dp(30)
            elevation:dp(6)
            radius:dp(20)
            size_hint:.9,None
            height:dp(200)
            pos_hint:{'center_x':.5}
            orientation:'vertical'
            MDBoxLayout:
                size_hint_y:.2
                # md_bg_color:.2,.5,.8,1
                MDLabel:
                    text:'Reset Password'
                    pos_hint:{'center_y':.2}
                    font_size:sp(25)
                    theme_text_color:'Custom'
                    text_color:app.theme_cls.primary_color
            MDBoxLayout:
                padding: ('0dp', '30dp', '0dp', '0dp')
                orientation: 'vertical'
                AnchorLayout:
                    anchor_y: 'center'
                    adaptive_height:True
                    MDTextField:
                        id:forgot_email
                        pos_hint:{'center_y':.5}
                        hint_text:"Enter your login email"
                
                AnchorLayout:
                    anchor_x:'right'
                    anchor_y:'bottom'
                    size_hint_y:1
                    MDRectangleFlatButton:
                        text:'Generate reset link'
                        on_press:app.forgot_password(forgot_email.text,reset_spinner)
                        # pos_hint:{'center_x':.5,'top':1}
                    FloatLayout:
                        size_hint_y:.1
                        
                        MDSpinner:        
                            id:reset_spinner
                            size_hint:None,None
                            size:dp(30),dp(30)  
                            active:False
                            pos_hint:{'center_y':.5,'center_x':.5}
        MDGridLayout:
            cols:1
            id:reset_link
            MDCard:
                orientation: 'vertical'
                size_hint:.9,None
                height:dp(150)

                pos_hint:{'center_x':.5}
                elevation:dp(6)
                radius:dp(20)
                padding:dp(20)
                MDLabel:
                    text:'Go to below link to reset your password'
                    theme_text_color:'Custom'
                    text_color:app.theme_cls.primary_color
                    font_size:sp(15)                
                    adaptive_height:True
                    size_hint_y:.5
                MDBoxLayout:
                    
                    MDLabel:
                        text:'asdfadssssssssssssssssssssssssssssssssssssasfasdfafasdfasdfas...'
                        pos_hint:{'center_y':.5}
                        font_size:sp(15)
                    ClickableMDIcon:
                        icon:'content-copy'
                        adaptive_size:True
                        pos_hint:{'center_y':.5}





