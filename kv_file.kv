                    source:'src/logo.png'
                    
            
        MDLabel:
            text:"Create Account"
            size_hint_y:.2
            font_name:login_font
            halign:'center'
            theme_text_color:'Custom'
            # text_color:0,0,1,1
            text_color:app.theme_cls.primary_color
            # text_color:app.theme_cls.primary_color
            font_size:sp(48)
            adaptive_height:True
        MDBoxLayout:
            # cols:1
            orientation: 'vertical'
            # md_bg_color:.8,.8,.2,1
            
            # padding:dp(50)
            padding: ('50dp', '10dp', '50dp', '30dp')
            MDBoxLayout:
                orientation:'vertical'
                # cols:1
                # md_bg_color:.8,.8,.8,1
                size_hint_y:.8
                pos_hint:{'center_y':.5}
                MDBoxLayout:
                    adaptive_height:True
                    spacing:dp(15)
                    
                    MDIcon:
                        icon:'account'
                        font_size:sp(20)
                        adaptive_size:True
                        pos_hint:{'center_y':.5}
                        theme_text_color:'Custom'
                        # text_color:0,0,1,1
                        text_color:app.theme_cls.primary_color
                    MDTextField:
                        id:register_user_name
                        hint_text:'Enter User Name'
                        helper_text:'No Spaces eg: User123.'
                        helper_text_mode:'on_focus'
                        adaptive_height:True
                        color_mode: 'custom'
                        # line_color_focus: 0, 0, 1, 1
                        line_color_focus:app.theme_cls.primary_color
                MDBoxLayout:
                   
                    adaptive_height:True
                    spacing:dp(15)
                    
                    MDIcon:
                        icon:'email'
                        font_size:sp(20)
                        adaptive_size:True
                        pos_hint:{'center_y':.5}
                        theme_text_color:'Custom'
                        # text_color:0,0,1,1
                        text_color:app.theme_cls.primary_color
                    MDTextField:
                        id:register_email
                        hint_text:'Enter email address'
                        adaptive_height:True
                        color_mode: 'custom'
                        # line_color_focus: 0, 0, 1, 1
                        line_color_focus:app.theme_cls.primary_color
                MDBoxLayout:
                    adaptive_height:True
                    spacing:dp(15)
                
                    ClickableMDIcon:
                        icon:'eye-off' if register_pass.password==True else 'eye'
                        adaptive_size:True
                        font_size:sp(20)
                        pos_hint:{'center_y':.5}
                        theme_text_color:'Custom'
                        # text_color:0,0,1,1
                        text_color:app.theme_cls.primary_color
                        # md_bg_color:.2,.2,.8,1
                        on_press:
                            new_val = not(register_pass.password)
                            register_pass.password =new_val
                                
                    MDTextField:
                        id:register_pass
                        helper_text:'At least 6 characters'
                        helper_text_mode:'on_focus'
                        hint_text:'Enter Password'
                        adaptive_height:True
                        password:True
                        color_mode: 'custom'
                        # line_color_focus: 0, 0, 1, 1
                        line_color_focus:app.theme_cls.primary_color
                MDBoxLayout:
                    adaptive_height:True
                    spacing:dp(15)
                    # md_bg_color:.5,.8,.8,1
                    ClickableMDIcon:
                        icon:'eye-off' if register_pass_confirm.password==True else 'eye'
                        adaptive_size:True
                        font_size:sp(20)
                        pos_hint:{'center_y':.5}
                        theme_text_color:'Custom'
                        # text_color:0,0,1,1
                        text_color:app.theme_cls.primary_color
                        # md_bg_color:.2,.2,.8,1
                        on_press:
                            new_val = not(register_pass_confirm.password)
                            register_pass_confirm.password =new_val
                                
                    MDTextField:
                        id:register_pass_confirm
                        hint_text:'Confirm Password'
                        helper_text:'At least 6 characters'
                        helper_text_mode:'on_focus'
