<Eidt_Fav>
    radius:dp(20)
    padding: ('0dp', '0dp', '0dp', '15dp')
    orientation: 'vertical'
    size_hint:1,None
    height:(app.w[1]/2.9)
    pos_hint:{'center_y':.5}
    MDBoxLayout:
        adaptive_height:True
        size_hint_y:.15
        MDLabel:
            text:'Edit Details'
            adaptive_height:True
            theme_text_color:'Custom'
            text_color:app.theme_cls.primary_color
            pos_hint:{'center_y':.8}
        MDLabel:
            text:'Edit Details'
            size_hint_x:.0001
            id:edit_key
            adaptive_height:True
            theme_text_color:'Custom'
            text_color:app.theme_cls.bg_light
            pos_hint:{'center_y':.8}
        MDLabel:
            text:'Edit Details'
            id:edit_category
            size_hint_x:.0001
            adaptive_height:True
            theme_text_color:'Custom'
            text_color:app.theme_cls.bg_light
            pos_hint:{'center_y':.8}
        MDIconButton:
            icon:'check-circle-outline'
            on_press:app.update_fav(edit_key.text,edit_category.text,edit_hint.text,edit_email.text,edit_pass.text)
            adaptive_height:True
    MDGridLayout:
        cols:2
        spacing:dp(10)
        MDIcon:
            icon:'comment-text-outline'
            size_hint_x:.15
        MDTextField:
            max_text_length: 20
            on_text: app.text_replace(self,20)
            id:edit_hint
            pos_hint:{'center_y':.5}
        MDIcon:
            icon:'email'
            size_hint_x:.15
        MDTextField:
            max_text_length: 20
            on_text: app.text_replace(self,20)
            id: edit_email
            pos_hint:{'center_y':.5}
        MDIcon:
            icon:'key'
            size_hint_x:.15
        MDTextField:
            max_text_length: 20
            on_text: app.text_replace(self,20)
            id:edit_pass
            pos_hint:{'center_y':.5}
        
