MDBoxLayout:
    MDCard:
        md_bg_color:app.theme_cls.bg_darkest
        orientation: 'vertical'
        padding: ('15dp', '10dp', '15dp', '15dp')
        spacing: dp(15)
        MDBoxLayout:
            size_hint:1,.15
            MDLabel:
                text:'Categories'
                theme_text_color:'Custom'
                text_color:app.theme_cls.primary_color
                font_name:'font/Austein.ttf'
                font_size:dp(48)
            MDCard:
                size_hint:.4,1
                MDIcon:
                    icon:'plus'
